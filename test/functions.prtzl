

Number one()
	return 1;
endfunc

Number add_one(Number x)
	return x + one();
endfunc

Number z = 3.1;

Number y = add_one(z);

Number w = add_one(3.1415);

String useful_function(String a, String b)
	return a ^ b;
endfunc

String hello = "hello ";
String world = "world";

String hello_world = useful_function(hello, world);